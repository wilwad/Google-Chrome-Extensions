{
  "manifest_version": 3,
  "name": "Index Links to Image",
  "description": "Index Links to Image",
  "version": "1.0",
  "action": {
    "default_popup": "html/default.html"
  },
"permissions": ["tabs", "scripting"],  
"host_permissions": ["https://*/*"],
"content_scripts": [
    {
      "js": ["scripts/content.js" ],
      "matches": [
        "https://*/*"
      ]      
    }
  ],
  "icons": {
    "16": "icons/16x16.png",
    "32": "icons/32x32.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"  	
  }
}
